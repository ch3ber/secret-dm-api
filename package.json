{
  "name": "secret-dm-api",
  "version": "0.0.1",
  "description": "API for secret DM",
  "main": "src/index.ts",
  "scripts": {
    "test": "NODE_ENV=test jest --detectOpenHandles",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "tsc": "tsc --project tsconfig.build.json && resolve-tspaths",
    "dev": "NODE_ENV=dev ts-node ./src/index",
    "start": "NODE_ENV=production node ./dist/index.js",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ch3ber/secret-dm-api.git"
  },
  "keywords": [
    "secret-dm"
  ],
  "author": "Eber Edrey Alejo Berrones <eber4202@hotmail.com> (https://ch3ber.github.io/)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ch3ber/secret-dm-api/issues"
  },
  "homepage": "https://github.com/ch3ber/secret-dm-api#readme",
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@faker-js/faker": "7.6.0",
    "@hapi/boom": "10.0.0",
    "@jest/globals": "29.3.1",
    "@types/express": "4.17.15",
    "@types/morgan": "1.9.4",
    "@types/node": "18.11.18",
    "@types/sequelize": "4.28.14",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "babel-jest": "29.3.1",
    "babel-plugin-module-resolver": "5.0.0",
    "eslint": "8.32.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-promise": "6.1.1",
    "husky": "8.0.0",
    "jest": "29.3.1",
    "resolve-tspaths": "0.8.3",
    "sequelize-cli": "6.5.2",
    "supertest": "6.3.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "joi": "17.7.0",
    "jsonwebtoken": "9.0.0",
    "morgan": "1.10.0",
    "pg": "8.8.0",
    "pg-hstore": "2.3.4",
    "sequelize": "6.28.0",
    "typescript": "4.9.4"
  }
}
